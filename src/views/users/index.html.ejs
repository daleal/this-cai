<div class="content">
  <h1>Usuarios</h1>

  <p>
    <a href="<%= newPath() %>">New</a>
  </p>

  <% users.forEach((user) => { %>
    <h2><%= user.firstName %> <%= user.lastName %></h2>
    <p>
      <%= user.email %><br>
      <%= user.phoneNumber %><br>
      Permisos: <%= user.role %>
    </p>
    <p>
      <a href="<%= showPath(user) %>">Show</a>
      <a href="<%= editPath(user) %>">Edit</a>
    </p>
    <p>
      <form action="<%= deletePath(user) %>" method="post">
        <input type="hidden" name="_method" value="delete">
        <input type="submit" value="Delete">
      </form>
    </p>
  <% }); %>
</div>
